/* components/detail/tabbar/tabbar.wxss */
.tabbar-box {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
}

.left, .middle, .right {
  flex: 1;
  text-align: center;
}

.left {
  justify-content: space-around;
}

.left .icon-xiangyou {
  font-size: 35rpx;
}

.middle {
  background: #f7c27d;
}

.right, .middle {
  height: 100%;
  line-height: 100rpx;
}